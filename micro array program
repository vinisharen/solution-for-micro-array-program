#include<stdio.h>
int main()
{
    int t,i;
    clrscr;
    scanf("%d",&t);
    for(i=0;i<t;i++)
    {
        int k,l;
        scanf("%d %d",&k,&l);
        int a=l;
        for(int j=0;j<k;j++)
        {
            int v;
            scanf("%d",&v);
            if(a>v)
            {
                a=v;
            }
        }
        printf("%d\n",l-a);
    }
    return 0;
}
